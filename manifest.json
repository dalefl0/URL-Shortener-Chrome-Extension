{
  "manifest_version": 3,
  "name": "URL Shortener test",
  "version": "1.0",
  "description": "Shortens the URL of the current tab",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
